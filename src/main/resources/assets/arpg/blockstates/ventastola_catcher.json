{
    "multipart": [
        {   "apply": { "model": "arpg:pane/ventastola_catcher_post" }},
        {   "when": { "north": true },
            "apply": { "model": "arpg:pane/ventastola_catcher_side" }
        },
        {   "when": { "east": true },
            "apply": { "model": "arpg:pane/ventastola_catcher_side", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "arpg:pane/ventastola_catcher_side_alt" }
        },
        {   "when": { "west": true },
            "apply": { "model": "arpg:pane/ventastola_catcher_side_alt", "y": 90 }
        },
        {   "when": { "north": false },
            "apply": { "model": "arpg:pane/ventastola_catcher_noside" }
        },
        {   "when": { "east": false },
            "apply": { "model": "arpg:pane/ventastola_catcher_noside_alt" }
        },
        {   "when": { "south": false },
            "apply": { "model": "arpg:pane/ventastola_catcher_noside_alt", "y": 90 }
        },
        {   "when": { "west": false },
            "apply": { "model": "arpg:pane/ventastola_catcher_noside", "y": 270 }
        }
    ]
}
